		function checkForm(searchForm) {
			var valid;
			if(searchForm.Query.value == "") {
				alert("Please Enter a Search Term");
				searchForm.Query.focus()
				valid = false;
			} else {
				valid = true;
				if(searchForm.bool.value == "all") {
					searchForm.Query.value = "\"" + searchForm.Query.value + "\"";
				}
			}
			return valid;
		}

