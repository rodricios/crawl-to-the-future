.imgPad { margin-top:5px; padding-right:5px; }
</style>

<script type="text/javascript">
function pipeLocalFeed(obj) {
    var x;
    var y =1;

   document.write("<div id='flRoll'>");

for (x = 0; x < obj.count ; x++) {

if (x==0)  {document.write('<div class="flHeader"><a href="/web/20100802013018/http://latimesblogs.latimes.com/movies/">24 Frames</a></div>');
document.write('<br>'); }

if (x==1)  {
document.write('<div class="dottedLine">&nbsp;</div>');
document.write('<br>');
document.write('<div class="flHeader"><a href="/web/20100802013018/http://latimesblogs.latimes.com/the_big_picture/">The Big Picture</a></div>');
}

if (x==2)  {
document.write('<div class="dottedLine">&nbsp;</div>');
document.write('<br>');
document.write('<div class="flHeader"><a href="/web/20100802013018/http://latimesblogs.latimes.com/entertainmentnewsbuzz/">Company Town</a></div>');
}

if (x==3)  {
document.write('<div class="dottedLine">&nbsp;</div>');
document.write('<div><a href="/web/20100802013018/http://theenvelope.latimes.com/"><img class="imgPad" src="/web/20100802013018/http://www.latimes.com/media/thumbnails/photo/2010-03/52547332-03143025.jpg" width="101" height="56" align="left" /></a></div>');
document.write('<br>');
document.write('<div class="flHeader"><a href="/web/20100802013018/http://theenvelope.latimes.com/">The Envelope</a></div>');
}

if (x==4) {
document.write('<div style="clear:both;"></div>');
document.write('<div class="dottedLine">&nbsp;</div>');
document.write('<div><a href="/web/20100802013018/http://latimesblogs.latimes.com/herocomplex/"><img class="imgPad" src="/web/20100802013018/http://latimesblogs.latimes.com/HeroComplexfestlogoThumb.jpg" width="101" height="56" align="left" /></a></div>');
document.write('<br>');
document.write('<div class="flHeader"><a href="/web/20100802013018/http://latimesblogs.latimes.com/herocomplex/">Hero Complex</a></div>');
}

  var buildstring = "<div class='flItem'>";
	buildstring +="<a href=" + obj.value.items[x].link + ">" + obj.value.items[x].title + "</a>";
	buildstring +="&nbsp\;<a href=" + obj.value.items[x].link + ">&raquo\;</a></div>";
	document.write(buildstring);

buildstring = null;
y++;
}

   document.write("</div>");
}
</script>

