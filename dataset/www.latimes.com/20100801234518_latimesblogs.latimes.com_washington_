<script type="text/javascript">
    // Tracking Tag iframe URL
    var VDNA_iframe_url = "/web/20100802030536/http://widgets-ak.visualdna.com/latimes.visualdna.com/live/itt/v1/impression_tracker.html",
    // Extract this window's parent URL and referer, to be sent to the iframe
    VDNA_parent_url = window.location.href,
    VDNA_referrer_url = (document.referrer == "" || document.referrer === null || document.referrer == "null") ? ((typeof(window.http_referrer) == "undefined" || window.http_referrer === null) ? "" : window.http_referrer) : document.referrer,
    VDNA_addLoadEvent = function(func){
        var oldonload = window.onload;
        if (typeof window.onload != 'function') {
            window.onload = func;
        } else {
            window.onload = function() {
                oldonload();
                func();
            };
        }
    },
    // Create the tracking iframe...
    VDNA_new_iframe = document.createElement("iframe"),
    VDNA_the_body;
    VDNA_new_iframe.setAttribute("style", "display: none;");
    VDNA_new_iframe.setAttribute("width", "1");
    VDNA_new_iframe.setAttribute("height", "1");
    VDNA_new_iframe.setAttribute("frameborder", "0");
    VDNA_new_iframe.setAttribute("src", VDNA_iframe_url + "?parent_url=" + VDNA_parent_url + "&ref=" + VDNA_referrer_url + ((typeof(VDNA_tracking_freq) != "undefined") ? ("&freq=" + VDNA_tracking_freq) : ""));
    VDNA_addLoadEvent(function() {
        //  ...and inject it at the end of the body
        VDNA_the_body = document.getElementsByTagName("body").item(0);
        VDNA_the_body.appendChild(VDNA_new_iframe);
    });
</script>
