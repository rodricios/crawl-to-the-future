<!-- Facebook -->
<div><iframe src="/web/20100802002519if_/http://www.facebook.com/widgets/like.php?href=http%3A%2F%2Flatimesblogs.latimes.com%2Ftechnology%2F2010%2F07%2Fmore-legal-reasons-to-circumvent-electronic-locks.html" scrolling="no" frameborder="0" style="border:none; width:225px; height:26px"></iframe></div>

