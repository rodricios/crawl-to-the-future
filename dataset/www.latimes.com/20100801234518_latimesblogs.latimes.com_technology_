		<script type="text/javascript">
			var t=jQuery("#root li a[href="+lowerTabPath+"]");
			if(t.length==0){t=jQuery("#root li a[href="+lowerTabPath+"/]");}
			if(t.length!=0){
				t=t.slice(0, 1);
				t.parent().attr("class", "highlight");
				t.parent().parent().attr("class", "level2 subStay");
				t.parent().parent().parent().attr("class", "navLink highlight");
