				t=jQuery("#root li a[href="+defaultTabPath+"]");
				if(t.length==0){t=jQuery("#root li a[href="+defaultTabPath+"/]");}				
				if(t.length!=0){
					t.parent().attr("class", "navLink highlight");
					t.parent().children("ul.level2").attr("class", "level2 subStay");
				}
			}
