function putFocus(formInst, elementInst) {
	if (document.forms.length > 0) {
		document.forms[formInst].elements[elementInst].focus();
	}
}

var submitted = false;

function preventDoubleClick() {
	if (submitted == false) {
		submitted = true;
		return true;
	}
	return false;
}

function Certify() {
	popupWin = window.open('/web/20050911224216/http://www.bbbonline.org/cnfm.cfm?company=03301999011123', 'Participant', 'location,scrollbars,width=450,height=300')
	window.name='opener';
}

function pop_me_up(pURL,features){ 
	new_window = window.open(pURL, "popup_window", features);
	new_window.focus();
}

function KeyPress(evt) {

	var keyCode;
	if (evt.which) {
		keyCode = evt.which;
	} else {
		keyCode = window.event.keyCode;
	}
	if (keyCode == 13) {
		if (evt.which) {
			evt.preventDefault();
		} else {
			window.event.keyCode = 0;
		}
	}
} 
