/* SCRIPT CONFIG */
var xsPopupOdds = 250;
var xsPopupMinVisits = 5;
var xsPopupSurvey = "/web/20051228031230/http://www.pdcsurveys.com/cgi-bin/p.pl?P=microsoft/4dd05c3b.htm";
var xsPopupWidth = "650";
var xsPopupHeight = "500";
var xsPopupDomain = "/web/20051228031230/http://shopping.msn.com/";

var xsPopupExpirationDate=new Date(new Date().getTime()+(31536000000)).toGMTString();
var today = new Date();
var xsPopupResults=document.cookie.match ('xsPopupVisits=(.*?)(;|$)');
var xsPopupResults2=document.cookie.match ('xsPopupSurveyCompletion=(.*?)(;|$)');
var xsPopupResults3=document.cookie.match ('xsPopupFirstVisit=(.*?)(;|$)');
var xsPopupResults4=document.cookie.match ('xsPopupLastVisit=(.*?)(;|$)');

if(xsPopupResults && xsPopupResults2 && xsPopupResults3 && xsPopupResults4){
	var xsPopupVisits=eval(xsPopupResults[1])+1;
	var xsPopupCompletion=eval(xsPopupResults2[1]);
	var xsPopupFirstVisit=xsPopupResults3[1];
	var xsPopupLastVisit=xsPopupResults4[1];
	var xsPopupRandom = Math.round(Math.random()*xsPopupOdds);
	if(xsPopupRandom == Math.round(xsPopupOdds/2) && xsPopupCompletion == 0){
		document.cookie= "xsPopupSurveyCompletion=1; expires=" +xsPopupExpirationDate + ";path=/";
		window.open(xsPopupSurvey,"_blank","width="+xsPopupWidth+",height="+xsPopupHeight+",top=" + ((screen.height - xsPopupHeight) / 2) + ", left=" + ((screen.width - xsPopupWidth) / 2) + ", scrollbars=yes,status=no,toolbar=no,menubar=no,location=no");
 	}
}
else {
	var xsPopupVisits=1;
	document.cookie= "xsPopupFirstVisit=" + today + "; expires=" +xsPopupExpirationDate + ";path=/";
	document.cookie= "xsPopupSurveyCompletion=0; expires=" +xsPopupExpirationDate + ";path=/";
}
document.cookie="xsPopupVisits="+xsPopupVisits+";expires="+xsPopupExpirationDate + ";path=/";
document.cookie= "xsPopupLastVisit=" + today + "; expires=" +xsPopupExpirationDate + ";path=/";
