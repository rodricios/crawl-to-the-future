<script language='javascript'>
<!---
function isStringEmpty4UM(StrInput) {
	var i, taille = StrInput.length;

	if (0 >= taille) return true;

	for (i = 0; i<taille; i++)
		if (' ' != StrInput.charAt(i)) return false;

	return true;
}
function UnpersonalizedSubmitValidation() {
	var aVariable, aConfirmationVariable, lb_SignSelected, li_DateSelected;

	lb_SignSelected = false;
	li_DateSelected = 0;

	aVariable = window.document.UnpersonalizedFormName.UnpersonalizedDaySelector[window.document.UnpersonalizedFormName.UnpersonalizedDaySelector.selectedIndex].value;
	if (false == isStringEmpty4UM(aVariable)) {
		li_DateSelected ++;
	}

	aVariable = window.document.UnpersonalizedFormName.UnpersonalizedMonthSelector[window.document.UnpersonalizedFormName.UnpersonalizedMonthSelector.selectedIndex].value;
	if (false == isStringEmpty4UM(aVariable)) {
		li_DateSelected ++;
	}

	aVariable = window.document.UnpersonalizedFormName.UnpersonalizedYearSelector[window.document.UnpersonalizedFormName.UnpersonalizedYearSelector.selectedIndex].value;
	if (false == isStringEmpty4UM(aVariable)) {
		li_DateSelected ++;
	}

	aVariable = window.document.UnpersonalizedFormName.UnpersonalizedSignSelector[window.document.UnpersonalizedFormName.UnpersonalizedSignSelector.selectedIndex].value;
	if (false == isStringEmpty4UM(aVariable)) {
		lb_SignSelected = true;
	}

	if (0 < li_DateSelected && true == lb_SignSelected) {
		alert('Please, choose between Zodiac sign and dates!');

		return false;
	}
	if (0 < li_DateSelected && 3 > li_DateSelected) {
		alert('Please, check your date!');

		return false;
	}
	if (0 == li_DateSelected && false == lb_SignSelected) {
	
		alert('Please choose between Zodiac sign or birth date!');


		return false;
	}

	return true;
}
function UnpersonalizedOnClick4Submit() {

	if (UnpersonalizedSubmitValidation()) {
		window.document.UnpersonalizedFormName.submit();
	}
}
function CleanDateChoice() {

	window.document.UnpersonalizedFormName.UnpersonalizedMonthSelector.selectedIndex=0;
	window.document.UnpersonalizedFormName.UnpersonalizedDaySelector.selectedIndex=0;
	
	window.document.UnpersonalizedFormName.UnpersonalizedYearSelector.selectedIndex=60;

}
//---></script>

