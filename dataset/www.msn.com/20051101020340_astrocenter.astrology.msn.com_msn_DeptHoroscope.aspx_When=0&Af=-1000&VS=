function isStringEmpty(StrInput) {
	var i, taille = StrInput.length;

	if (0 >= taille) return true;

	for (i = 0; i<taille; i++)
		if (' ' != StrInput.charAt(i)) return false;

	return true;
}



function SubmitValidation() {
	var aVariable, aConfirmationVariable, aBirthTimeUnkownFlag = false, aField, lb_DiplayDialogBox = true;

	aField = eval('window.document.PZ_MainForm.DisplayDialogBox');				
	if (typeof(aField) != 'undefined') {
		lb_DiplayDialogBox = (aField.value == 'true');
//		alert('lb_DiplayDialogBox=' + aField.value);
	}

	
		var aField = eval('window.document.PZ_MainForm.FirstName');
		if (typeof(aField) != 'undefined') {
			aVariable = window.document.PZ_MainForm.FirstName.value;
			if (isStringEmpty(aVariable) && lb_DiplayDialogBox) {
				alert("Please, enter your first name");
				return false;
			}
		}
	
		aField = eval('window.document.PZ_MainForm.DaySelector.selectedIndex');
		if (typeof(aField) != 'undefined') {
			aVariable = window.document.PZ_MainForm.DaySelector[window.document.PZ_MainForm.DaySelector.selectedIndex].value;
			if (isStringEmpty(aVariable) && lb_DiplayDialogBox) {
				alert("Please choose a day");
				return false;
			}
		}
	
		aField = eval('window.document.PZ_MainForm.MonthSelector.selectedIndex');
		if (typeof(aField) != 'undefined') {
			aVariable = window.document.PZ_MainForm.MonthSelector[window.document.PZ_MainForm.MonthSelector.selectedIndex].value;
			if (isStringEmpty(aVariable) && lb_DiplayDialogBox) {
				alert("Please, choose a month");
				return false;
			}
		}
	
		aField = eval('window.document.PZ_MainForm.YearSelector.selectedIndex');
		if (typeof(aField) != 'undefined') {
			aVariable = window.document.PZ_MainForm.YearSelector[window.document.PZ_MainForm.YearSelector.selectedIndex].value;
			if (isStringEmpty(aVariable) && lb_DiplayDialogBox) {
				alert("Please, choose a year of birth");
				return false;
			}
		}
	

	if (typeof(CustomizedSubmitValidation)=="function") return CustomizedSubmitValidation();

	

	return true;
}
function OnClick4Submit(EtatAutomate) {

	if (SubmitValidation()) {
		document.PZ_MainForm.AutomateState.value = EtatAutomate;
		document.PZ_MainForm.submit();
	}
}
