	alert('Thank you for visiting the MSN Astrology site. We\'re sorry, but you must be 13 or older to personalize your horoscope. Please visit MSN Kids (/web/20051028204516/http://kids.msn.com) where you\'ll find lots of fun games and other cool things to do.');
}
function getIntValue(aFieldName) {

	var aField = eval('window.document.' + aFieldName);				

	return parseInt(aField.value, 10);
}
function CustomizedSubmitValidation() {
	var theLimit = 20051028 - 130000, theAge = 0;
	var ReturnValue = true;



	// Clear the COPPA Cookie
	SetCookie('COPPA', 'false', null, '/', 'astrology.msn.com', false);

	var theYear = 0, theMonth = 0, theDay = 0;
	var aFieldName;
	var aFormName;
	
	// Cas de la page Default.aspx
	
	aFieldName = 'PZ_MainForm' + '.' + 'YearSelector';
	theYear = getIntValue(aFieldName + '[' + aFieldName +'.selectedIndex]');
	aFieldName = 'PZ_MainForm' + '.' + 'MonthSelector';
	theMonth = getIntValue(aFieldName + '[' + aFieldName +'.selectedIndex]') + 1;
	aFieldName = 'PZ_MainForm' + '.' + 'DaySelector';
	theDay = getIntValue(aFieldName + '[' + aFieldName +'.selectedIndex]');
	
	
	theAge = theYear * 10000 + theMonth * 100 + theDay;
	if (theAge > theLimit) ReturnValue = false;

	theAge = 0;
	aFormName = 'SunSignForm';
	if ('undefined' != typeof(SunSignForm)) {
		if ('undefined' != typeof(SunSignForm.MonthSelector)) {
			aFieldName = 'YearSelector';
			theYear = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
			aFieldName = 'MonthSelector';
			theMonth = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
			aFieldName = 'DaySelector';
			theDay = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');

			theAge = theYear * 10000 + theMonth * 100 + theDay;
			if (theAge > theLimit) ReturnValue = false;
		}
	}
	
	theAge = 0;
	aFormName = 'LoveSignForm';
	if ('undefined' != typeof(LoveSignForm)) {
		// Manque de bol, le lovesignform sert dans plusieurs formulaires??
		if ('undefined' != typeof(LoveSignForm.PHSF_MonthSelector)) {
			aFieldName = 'PHSF_YearSelector';
			theYear = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
			aFieldName = 'PHSF_MonthSelector';
			theMonth = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
			aFieldName = 'PHSF_DaySelector';
			theDay = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');

			theAge = theYear * 10000 + theMonth * 100 + theDay;
			if (theAge > theLimit) ReturnValue = false;
		}
		if ('undefined' != typeof(LoveSignForm.PLSF_MonthSelector)) {
			aFieldName = 'PLSF_YearSelector';
			theYear = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
			aFieldName = 'PLSF_MonthSelector';
			theMonth = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
			aFieldName = 'PLSF_DaySelector';
			theDay = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');

			theAge = theYear * 10000 + theMonth * 100 + theDay;
			if (theAge > theLimit) ReturnValue = false;
		}
	}
			
	theAge = 0;
	aFormName = 'ChineseSignForm';
	if ('undefined' != typeof(ChineseSignForm)) {
		aFieldName = 'PCSF_YearSelector';
		theYear = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
		aFieldName = 'PCSF_MonthSelector';
		theMonth = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
		aFieldName = 'PCSF_DaySelector';
		theDay = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');

		theAge = theYear * 10000 + theMonth * 100 + theDay;
		if (theAge > theLimit) ReturnValue = false;
	}

	theAge = 0;
	aFormName = 'NumerologyForm';
	if ('undefined' != typeof(NumerologyForm)) {
		aFieldName = 'PNSF_YearSelector';
		theYear = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
		aFieldName = 'PNSF_MonthSelector';
		theMonth = getIntValue(aFormName + '.' + aFieldName + '[' + aFormName + '.' + aFieldName +'.selectedIndex]');
