    var path = location.pathname;
    var query = location.search ;
    if (query == '') {
      query = "?need=prefs";
    } else {
      query += "&need=prefs";
    }
    var cfu = escape("/web/20051103025617/https://s.foxsports.com/pp/response?cfu=" + path + query);
    var queryParams = "?seclog=10&wa=wsignin1.0&wreply=" + cfu + "&cb=" + escape("&wa=wsignin1.0&wreply=") + cfu;
    return "/web/20051103025617/https://login.passport.com/ppsecure/secure.srf" + queryParams;
  }
