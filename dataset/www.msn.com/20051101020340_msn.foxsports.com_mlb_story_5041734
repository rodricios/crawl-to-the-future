<script language="javascript">
  function _hbxStrip(a){
    a = a.replace(/\s+$/, '');
    a = a.split(",").join("");
    a = a.split("&").join("");
    a = a.split("'").join("");
    a = a.split("#").join("");
    a = a.split("$").join("");
    a = a.split("%").join("");
    a = a.split("^").join("");
    a = a.split("*").join("");
    a = a.split(":").join("");
    a = a.split("!").join("");
    a = a.split("<").join("");
    a = a.split(">").join("");
    a = a.split("~").join("");
    a = a.split(";").join("");
    a = a.split("\\").join("");
    a = a.split(" ").join("+");
    return a;
  }
