<script>
function shortenString(stringValue) {
  //Shortens a 14+ char string into a 14 char string
  if(stringValue.length > 13) {
    var stringValueShortened = stringValue.substr(0,14);
    stringValueShortened = stringValueShortened + "...";
    return stringValueShortened;
  }
  else
    return stringValue;
}
