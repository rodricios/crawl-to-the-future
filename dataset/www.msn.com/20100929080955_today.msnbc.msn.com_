jQuery.fn.extend({
    isAboveViewport: function (offset) {
        var t = $(window).scrollTop();
        return t >= this.offset().top + this.height() + (offset || 0);
    },
    isBelowViewport: function (offset) {
        var b = $(window).height() + $(window).scrollTop();
        return b <= this.offset().top + (-offset || 0);
    },
    isInViewport: function (padding) {
        return !this.isAboveViewport(padding) && !this.isBelowViewport(padding);
