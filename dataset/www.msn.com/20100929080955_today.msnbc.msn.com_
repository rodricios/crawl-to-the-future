            "text": LOADING_TEXT
        }).appendTo(gridSlot);

        sections.find("li").each(function (index) {
            var li = $(this);
            var gid = getGridId(li);
            var delay;
            $(this).bind("mouseover", function () {
                delay = setTimeout(function () {
                    root.removeClass('s' + currentIndex);
                    handleSectionTrigger(li, gid, index);
                }, HOVER_DELAY);
            }).bind("mouseout", function () {
                clearTimeout(delay);
            });
        });
    };

    function handleSectionTrigger(li, gridId, index) {
        if (!gridId || gridId === currGridId) {
            return;
        }
        index = index + 1;
        currGridId = gridId;
        root.addClass('s' + index);
        currentIndex = index;
        getData(
        gridId, function (d) {
            data = d;
            gridSlot.html(data);
            addMultimediaIcon();
        });
    };

    function addMultimediaIcon() {
  var upscrollGrid = $('#upscroll-grid');
  var videoLinks = $('.story h6 a[href*=vPlayer], .story h6 a[href*=/vp/]', upscrollGrid);   
  for (var i=0; i < videoLinks.length; i++) {
   $(videoLinks[i]).prepend(videoIconHtml);
  };   
  var videoLinksH4 = $('.story h4 a[href*=vPlayer], .story h4 a[href*=/vp/]', upscrollGrid);
  console.log("TRACE: videoLinksH4 = ", videoLinksH4);
  for (var i=0; i < videoLinksH4.length; i++) {
   $(videoLinksH4[i]).parent().next().prepend(videoIconHtml);
   
   console.log("TRACE: $(videoLinksH4[i]).next() = ", $(videoLinksH4[i]).next());
  };       
  var slideShowLinks = $('.story h6 a[href*=SSOpen]', upscrollGrid);
  for (var i=0; i < slideShowLinks.length; i++) {
   $(slideShowLinks[i]).prepend(photoIconHtml);
  };   
  var slideShowLinksH4 = $('.story h4 a[href*=SSOpen]', upscrollGrid);
  for (var i=0; i < slideShowLinksH4.length; i++) {
   $(slideShowLinksH4[i]).parent().next().prepend(photoIconHtml);
  };   
  
