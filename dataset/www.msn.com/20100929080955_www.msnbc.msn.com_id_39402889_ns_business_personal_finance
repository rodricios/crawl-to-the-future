<script type="text/javascript">
    $(document).ready(function() {
        var bizform = $('#bizsearch');
        var query = bizform.attr('data-query');
        ijv.features.StockSearch.changeInput(bizform.find('input.q'));
        ijv.features.StockSearch.getData(query, function(d) {
            //ijv.log(query, d);
            if (d[query]) {
                bizform.find('.arrow').removeClass('arrow').addClass(d[query].arrow);
                bizform.find('.stock_diff a').html(d[query].change);
            }
        });
    });
</script>
