			function nextStory(){
				var sStoryArray = "9856550^Seven U.S. troops killed in Iraq violence|9875157^NBC: Berlusconi expects frosty welcome|9874913^Iraq troop pullout tops Italian PM's agenda|9877430^NBC: Syria shows off border with Iraq|9868627^Many talented Iraqis are leaving|9844568^WP: Kurds attempt to reclaim prized city|9864433^Did Saddam accept exile offer before war?|9847407^Saddam co-defendant pleads for treatment|9833759^Report: Oil-for-food fraud widespread|9772398^Death toll for U.S. in Iraq hits 2,000|";
				if(sStoryArray.charAt(sStoryArray.length)=="|"){
					sStoryArray = sStoryArray.substr(0,sStoryArray.length-1);
				}
				var aStoryArray = sStoryArray.split("|");
				if(window.partID){}else{partID='';}
				if (curDoc != '' && sStoryArray.indexOf(curDoc)>-1){
					curDom = document.getElementById(curDoc);
					curDom.style.backgroundColor = '#dddddd';
					curDomIH = curDom.innerHTML;
					curDom.innerHTML = curDomIH.substr(0,52) + '<img src="/images/arrowCurrentStory.gif" border="0" width="16" height="16">' + curDomIH.substr(52,curDomIH.length);
					for (i = 0; i < aStoryArray.length; i++) {
						var aCurStory = aStoryArray[i].split("^");
						var storyHref = aCurStory[0];
						var storyHL = aCurStory[1];
						if(storyHref == curDoc){
							var nxtStory = aStoryArray[i+1];
						}
					}
					if (nxtStory){
						var nxtStoryArray = nxtStory.split("^");
						var storyHref = nxtStoryArray[0];
						if (!isNaN(storyHref)){
							var storyHL = nxtStoryArray[1];
							if (partID){
								var nxtHref = 'href="/id/'+partID+'/did/'+storyHref+'"';}
							else{
								var nxtHref = 'href="/id/'+storyHref+'/"';}
							var nxtText = document.getElementById("nextText");
							var nxtButton = document.getElementById("StoryNextButton");
							var replaceStart = nxtButton.innerHTML.indexOf("href");
							nxtText.innerHTML = storyHL;
							nxtButton.innerHTML = nxtButton.innerHTML.replace(nxtButton.innerHTML.substring(replaceStart,replaceStart+19),nxtHref);
						}
					}
				}
			}
			nextStory();
