}

try
{
  
    if (HasFlashVersion(6) == true && g_TestNoFlash == false)
    {
      writeFlashEx("420",
        "225",
        "Superblip",
        "/images/v9/en-us/game/superblipplayer4.swf",
        "/images/v9/en-us/game/",
