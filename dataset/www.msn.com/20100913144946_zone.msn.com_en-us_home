function WriteAltFlash()
{
document.writeln('<DIV>');
document.writeln("<a href='/web/20100914075805/http://zone.msn.com/en/bubbletown/default.htm'><img src='/web/20100914075805/http://cdn1.zone.msn.com/images/v9/en-us/game/bubl/sblip225_bubl.jpg' alt='Bubble Town' border='0' /></a>");
