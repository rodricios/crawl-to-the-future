adsyncnextadarr = new Array();
adsyncdefaultmap= new Array();
function ao_adSync(pagegroupid)
{
 var adsync_isSponsored = false;
 for(var i in adsyncmap) {
  var p = i.split(0);
  if( p == pagegroupid)
  {
   adsync_isSponsored = true;

   break;
  }//end if sponsored
 }// end for loop
 
 if (adsync_isSponsored == true)
 {
  adsyncnextadarr = adsyncmap;
  adsyncMakeAd(adsyncnextadarr);
  $("#bantopBadge").css("display","block");
 }else{
  if(typeof(adsyncdefaultmap) != 'undefined')
  {
   adsyncnextadarr = adsyncdefaultmap;
   adsyncMakeAd(adsyncnextadarr);
   $("#bantopBadge").css("display","none");
  }
 }// end is sposored loop
}//end adsync function
function as_createDefaultMap(as_dm_entry)
{
	var as_dm_entry_split = as_dm_entry.split(",");

	 try{
		 adsyncdefaultmap[as_dm_entry_split[1]]=as_dm_entry_split[0]+',"&PG='+as_dm_entry_split[1]+'&AP='+as_dm_entry_split[2]+'",'+as_dm_entry_split[3]+','+as_dm_entry_split[4]+'';
		 adsyncMakeAd(adsyncdefaultmap);
	 }catch(err){
	 	var x = err.description;
	 }
}// end as_createDefaultMap
function adsyncMakeAd(nxtadarr)
{
 for(var i in nxtadarr) {
  var asvalue = nxtadarr[i]
  adsync_split = asvalue.split(",");
  adsync_nextAd='dapMgr.renderAd("'+adsync_split[0]+'",'+adsync_split[1]+','+adsync_split[2]+','+adsync_split[3]+',1)';
  adsyncdiv='.'+adsync_split[0];
  $(adsyncdiv).empty();
  var asyncAdScript=document.createElement('script'); 
  asyncAdScript.type='text/javascript'; 
  asyncAdScript.text=adsync_nextAd;
  $(adsyncdiv).append(asyncAdScript);
 }// end for loop
  
 if($.browser.mozilla){
  var adsyncto=setTimeout('dapMgr.trackEvent(eventType.click)',2000);
 }//end if mozilla 
}//end adsyncMakeAd function
