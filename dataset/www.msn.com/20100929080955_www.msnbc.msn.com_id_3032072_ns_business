    var names = ["log", "debug", "info", "warn", "error", "assert", "dir", "dirxml", "group", "groupEnd", "time", "timeEnd", "count", "trace", "profile", "profileEnd"];
    window.console = {};
    for (var i = 0; i < names.length; ++i) window.console[names[i]] = function () {};
}

var IS_IE = $.browser.msie;
var IEdebug = null;
// IE temp debug console
if (IS_IE && IEdebug) {
    $('body').append("<div id='IEconsole' style='position:absolute; z-index: 1000; top:580px; right:0; width:200px; background:#EECCCC;'>IE Debug Console -v " + IEdebug + "<\/div>");
}
this.echo = function (msg, clear) {
    var curContent = $('#IEconsole').html() + "<br />";
    if (clear) {
        curContent = '';
    }
    $('#IEconsole').html(curContent + msg);
}

//console.log("jQuery is ready:" , $);
// Utils 
jQuery.fn.extend({
    isAboveViewport: function (offset) {
        var t = $(window).scrollTop();
        return t >= this.offset().top + this.height() + (offset || 0);
    },
    isBelowViewport: function (offset) {
        var b = $(window).height() + $(window).scrollTop();
        return b <= this.offset().top + (-offset || 0);
    },
    isInViewport: function (padding) {
        return !this.isAboveViewport(padding) && !this.isBelowViewport(padding);
    }
});

// features   
var gridswitcher = (function (root) {
    var LOADING_TEXT = "loading latest headlines ...";
    var CLASS_LOADING = "loading";
    var HOVER_DELAY = 250;
    var currGridId = 0;
    var gridSlot = $("#upscroll-grid");
    var sections = $('#sections');
    var loaded = false;
    var loadingIndicator = null;
    var data = null;
    var currentIndex = 1;
 
 var videoIconHtml = '<span class="icon"><span class="video"></span></span>';
 var photoIconHtml = '<span class="icon"><span class="photo"></span></span>';

    function init() {
        if (!gridSlot.length) {
            return;
        }
        currGridId = getGridId(sections.find("li:first"));
        getData(
        currGridId, function (d) {
            data = d;
            loadingIndicator.remove();
            gridSlot.html(data);
   addMultimediaIcon();
            loaded = true;
        });
        setupBehavior();
    };

    function setupBehavior() {
        loadingIndicator = $("<div/>", {
            "class": CLASS_LOADING,
            "text": LOADING_TEXT
        }).appendTo(gridSlot);

        sections.find("li").each(function (index) {
            var li = $(this);
            var gid = getGridId(li);
            var delay;
            $(this).bind("mouseover", function () {
                delay = setTimeout(function () {
                    root.removeClass('s' + currentIndex);
                    handleSectionTrigger(li, gid, index);
                }, HOVER_DELAY);
            }).bind("mouseout", function () {
                clearTimeout(delay);
            });
        });
    };

    function handleSectionTrigger(li, gridId, index) {
        if (!gridId || gridId === currGridId) {
            return;
        }
        index = index + 1;
        currGridId = gridId;
        root.addClass('s' + index);
        currentIndex = index;
        getData(
        gridId, function (d) {
            data = d;
            gridSlot.html(data);
            addMultimediaIcon();
        });
    };

    function addMultimediaIcon() {
  var upscrollGrid = $('#upscroll-grid');
  var videoLinks = $('.story h6 a[href*=vPlayer], .story h6 a[href*=/vp/]', upscrollGrid);   
  for (var i=0; i < videoLinks.length; i++) {
   $(videoLinks[i]).prepend(videoIconHtml);
  };   
  var videoLinksH4 = $('.story h4 a[href*=vPlayer], .story h4 a[href*=/vp/]', upscrollGrid);
  console.log("TRACE: videoLinksH4 = ", videoLinksH4);
  for (var i=0; i < videoLinksH4.length; i++) {
   $(videoLinksH4[i]).parent().next().prepend(videoIconHtml);
   
   console.log("TRACE: $(videoLinksH4[i]).next() = ", $(videoLinksH4[i]).next());
  };       
  var slideShowLinks = $('.story h6 a[href*=SSOpen]', upscrollGrid);
  for (var i=0; i < slideShowLinks.length; i++) {
   $(slideShowLinks[i]).prepend(photoIconHtml);
  };   
  var slideShowLinksH4 = $('.story h4 a[href*=SSOpen]', upscrollGrid);
  for (var i=0; i < slideShowLinksH4.length; i++) {
   $(slideShowLinksH4[i]).parent().next().prepend(photoIconHtml);
  };   
  
        // if ($("#todayShow").length > 0) {
        //     console.log("TRACE: add multimedia icons for today show");
        // } else {
        //     console.log("TRACE: add multimedia icons for msnbc");
        // }
    }

    function getGridId(el) {
        return parseInt(el.attr('grid'), 10);
    };

    function getData(gridId, callback) {
        var self = this;
        //var url = String(window.location.href).split("/id/")[0] + "/id/" + gridId;
        var url = String(window.location.href).split(".com/")[0] + ".com/id/" + gridId;
        jQuery.ajax({
            "dataType": "html",
            "url": url,
            "success": function (data) {
                if (data && callback) {
                    callback.call(self, data);
                }
            }
        });
    };

    init();
    return {};
});
var upscroll = (function () {
    var ID_NET = "network";
    var ID_SITEHEADER = "siteHeader";
    var ID_NAV = "site";
    var ID_MARQUEE = "marqueeContainer";
    var CLASS_CONTENT = ".main";
    var SCROLLTOP_WHERE_75_PERCENT_IN_VIEW = 100; // for analytics
    //public parts
    var MY = {};

    MY.init = function (jq_rootElement, type, data) {
        // Fix non SPF breadcrumb
        var breadcrumb = $("#breadcrumbs .breadcrumbs li");

        for (var i = 0; i < breadcrumb.length; i++) {
            if (i == breadcrumb.length - 1) {
                $(breadcrumb[i]).css({
                    "display": "inline"
                });
            } else {
                $(breadcrumb[i]).css({
                    "display": "none"
                });
            }

        };
 $('#todayShow #breadcrumbs').contents().filter(function() { return this.nodeType == 3; }).remove(); 

        // disable IE 6
        if ("undefined" === typeof document.body.style.maxHeight) {
            return;
        }

        // HACK PART 1: to add the missing 63px to the navContent div height
        // The SPF upscroll calculates the navContent height properly, while
        // the non SPF one doesn't. This could be because non SPF upscroll
        // is rendering in quirkmode. 
        // HACK PART 2: The 227 px comes from the height of the grid. We removed 
        // the grid css height initially, so it won't push the logo below the cover
        //  before the JS finish the upscrolling. We will add the css height back later 
        //  after the JS is done calculating
        var NAV_CONTENT_ADDITIONAL_HEIGHT = 63 + 227; // 235 come from the height of the grid
        this.root = $(jq_rootElement);
        this.net = $("#" + ID_NET);
        this.nav = $("#" + ID_NAV);
        this.siteHeader = $("#" + ID_SITEHEADER);
        this.marquee = $("#" + ID_MARQUEE);
        this.sections = $("#sections li");
        this.navContent = this.nav.find(CLASS_CONTENT);
        this.netHeight = this.net.height();
        this.navHeight = this.nav.height();
        this.marqueeHeight = this.marquee.height();

        if ($("#todayShow").length > 0) {
            this.hdrHeight = this.navContent.height() + this.netHeight + this.marqueeHeight + NAV_CONTENT_ADDITIONAL_HEIGHT;
        } else {
            this.hdrHeight = this.navContent.height() + this.netHeight + this.marqueeHeight + NAV_CONTENT_ADDITIONAL_HEIGHT - 35;
        }

        this.defHeight = this.navHeight + this.netHeight + this.marqueeHeight;
        this.upHeight = this.hdrHeight - this.defHeight;
        this.netIsFixed = false;
        this.hasExtendedOnce = false;
        // HACK PART 3: set the height of the grid after the JS finish the upscroll calculation
        // For Non IE
        if (!IS_IE) {
            //$("#siteHeader .main #upscroll-grid").css("height","235px");
            if ($("#todayShow").length > 0) {
                $("#siteHeader .main #upscroll-grid").css({
                    "height": "235px"
                });
            } else {
                $("#siteHeader .main #upscroll-grid").css({
                    "height": "200px"
                });
            }

            this.reset();
            if (this.root.isInViewport()) {
                $("html,body").scrollTop(this.upHeight);
            }
            this.bindEvents();
        } else {
            //For IE
            //this.siteHeader.css({"margin-top":"5"});      
            var self = this;
            $(window).scrollTop(5);
            $(window).scroll(function (e) {
                if ($(window).scrollTop() == 0 && !this.hasExtendedOnce) {
                    if ($("#todayShow").length > 0) {
                        $("#siteHeader .main #upscroll-grid").css({
                            "height": "235px"
                        });
                        self.siteHeader.css({
                            "height": "342px"
                        });
                        self.nav.css({
                            "height": "318px"
                        });
                    } else {
                        self.siteHeader.css({
                            "height": "307px"
                        });
                        self.nav.css({
                            "height": "283px"
                        });
                        $("#siteHeader .main #upscroll-grid").css({
                            "height": "200px"
                        });
                    }
     $("#tbx-37650045").css({"zoom":"1"});
                    // $("#siteHeader .main #upscroll-grid").css({"height":"235px"});
                    this.hasExtendedOnce = true;
                    //$("#siteHeader #network").css({"padding-top":"0px"});
                    //activate grid switching
                    var switcher = gridswitcher(self.navContent);
                }
            });
            // Init the more nav
            //console.log("TRACE: $(more) = ", $("#msn .more"));
            $("#msn .more").mouseover(function (e) {
                $(this).addClass("moreOut");
            });
            $("#msn .more").mouseout(function (e) {
                $(this).removeClass("moreOut");
            });
        }
    };
    MY.reset = function () {
        this.root.height(this.hdrHeight);
        this.nav.height(this.navHeight);
        this.nav.css("top", this.upHeight);
        this.net.css("top", this.upHeight);
        this.navContent.css("top", this.upHeight * -1);
        this.net.css({
            "position": "relative"
        });
        this.marquee.css({
            "position": "relative",
            "top": this.upHeight
        });
        this.netIsFixed = false;
    };
    MY.update = function () {
        var scrollTop = $(window).scrollTop();
        if (scrollTop > this.upHeight) {
            if (this.netIsFixed) {
                this.reset();
            }
            return;
        }

        if (scrollTop > this.upHeight) {
            if (this.netIsFixed) {
                this.reset();
            }
            return;
        }

        if (this.netIsFixed == false) {
            this.net.css({
                "top": 0,
                "position": "fixed"
            });
            this.marquee.css({
                "top": this.netHeight,
                "position": "fixed"
            });
            this.netIsFixed = true;
        }

        this.nav.css({
            "height": this.hdrHeight - scrollTop - this.netHeight - this.marqueeHeight,
            "top": scrollTop + this.netHeight + this.marqueeHeight
        });

        this.navContent.css("top", scrollTop * -1);

        if (scrollTop < SCROLLTOP_WHERE_75_PERCENT_IN_VIEW && !this.hasExtendedOnce) {
            //console.log("TRACE: gridSwitcher created");
            this.hasExtendedOnce = true;
            // activate grid switching
            var switcher = gridswitcher(this.navContent);
            var self = this;
        };
    };
    MY.bindEvents = function () {
        var self = this;
        var doCheck = true;
        setTimeout(function () {
            doCheck = false;
        }, 800);
        $(window).scroll(function (e) {
            if (doCheck && self.root.isInViewport() /*!jQuery.support.cssFloat &&*/ ) {
                $("html,body").scrollTop(self.upHeight);
            }
            self.update();
            doCheck = false;
        });
        //if (jQuery.support.cssFloat) {
        setTimeout(function () {
            self.update();
            if (self.root.isInViewport()) {
                $("html,body").scrollTop(self.upHeight);
            }
        }, 500);
        //}
    };
    return MY;
})();
$(document).ready(function () {
    upscroll.init($("#siteHeader"));


