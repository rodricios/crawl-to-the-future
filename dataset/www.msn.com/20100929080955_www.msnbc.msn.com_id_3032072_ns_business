function removeAdPadding(){
  var ad=document.getElementById('sliderAd');
if(ad==null) return;
  var holder=document.getElementById('sliderAdHolder');
  var foo = ad.innerHTML.indexOf('#sliderAd {display:none');

  if(foo!=(-1)){holder.style.padding=0;}
  else{
holder.style.paddingTop=4; 
holder.style.paddingBottom=4;
}
}

