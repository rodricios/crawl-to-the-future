	var displayConfig = {
	    "noDataText":"We don't have suggestions that fit your travel plans. To see all results for your current flight criteria, click Search flights.",
		"yourSearchText":"Your Flight Summary",	
		"otherOptionsText":"Consider these options",
		"SearchLinkText":"Search Flights"
	};

	var tracker = new FC.MetaSearch.Blink.SuggestedSearches.Tracker();
    var formExtractor = new FC.MetaSearch.Blink.Form("flight_search_form","leave1", "return1", "stops","no","ne","cabin","adults");
    var searchLinker = new FC.MetaSearch.Blink.SuggestedSearches.SearchLinker("/flight/flightSearch", tracker);
    var queryExecutor = new FC.MetaSearch.Blink.SuggestedSearches.QueryExecutor("/travel/data/blink/air");
    var displayManager = new FC.MetaSearch.Blink.SuggestedSearches.DisplayManager("rightRail", searchLinker, displayConfig, tracker);
    var blinkManager = new FC.MetaSearch.Blink.SuggestedSearches.Manager(formExtractor, queryExecutor, displayManager);   
    var eventListener = new FC.MetaSearch.Blink.EventListener("orig1Text", "dest1Text", "leave1", "return1", "stops","no","ne","cabin","adults");
    eventListener.register(blinkManager);
    
    function OnClickFlightSearch() {
    	displayManager.setValidBlinkForm(true);
	}
	
	function OnClickHotelSearch() {
		displayManager.setValidBlinkForm(false);
	}
</script>


	
            
				




