$(function() {
	var createSetSelectedFunction = function(selectedId) {
		return function(){
			$('.journeyControl .selected').removeClass('selected');
			$('#'+selectedId).addClass('selected');
		};
	}
	// event attachment
	if ($('#labelRT').length > 0 && $('#labelOW').length > 0 && $('#labelMC').length > 0) {
		$('#labelRT').click(function() {setTimeout(createSetSelectedFunction('labelRT'), 50); return false;});
		$('#labelOW').click(function() {setTimeout(createSetSelectedFunction('labelOW'), 50); return false;});
		$('#labelMC').click(function() {setTimeout(createSetSelectedFunction('labelMC'), 50); return false;});
	}
});

$(function() {
	$('#orig1Text').focus();
});
