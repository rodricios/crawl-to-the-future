<script language="JavaScript1.2" type="text/javascript">
  function checkCookie(cookieName, length) {
  var allcookies = document.cookie;
  var cookiePos = allcookies.indexOf(cookieName + "=");
  if(cookiePos != -1) {
    var start = cookiePos + length;
    var end = allcookies.indexOf(";", start);
    if(end == -1)
      end = allcookies.length;
    var value = allcookies.substring(start, end);
    value = unescape(value);
    return value;
  }
  return "";
  }
  var uid;
  var lmd = checkCookie("LMD", 4);
  if (lmd != null && lmd != "") {
    uid = checkCookie("UID", 4);
  } else {
    uid = checkCookie("RME", 4);
    if (uid != null && uid != "") {
      var curDate = new Date();
      curDate = curDate.getMilliseconds();
      document.cookie = "LMD=" + curDate + "; path=/";
      document.cookie = "UID=" + uid + "; path=/";
    }
  }
  document.write('<scr' + 'ipt language="JavaScript1.2" src="/user.js?uid=' + uid + '&lmd=' + escape(lmd) + '" type="text/javascript"></scr' + 'ipt>');
</script><!--start hitbox code-->

