function cnvrtQuote2List(aStocks,nStart,nEnd) {

 var quoteRedir = "/web/20051102074652/http://moneycentral.msn.com/investor/research/msnbc/newsnap.asp";

var shortNames = new Array();
shortNames["$INDU"] = "DJIA";
shortNames["$COMPX"] = "NASDAQ";
shortNames["$INX"] = "S&P 500";
shortNames["$IUX"] = "Russell";
shortNames["$TRAN"] = "DJTA";
shortNames["$UTIL"] = "DJUA";
shortNames["TC10Y"] = "10-year";

 aTmp = new Array(); 
 if (!nStart) { nStart = 0; }
 if (!nEnd) { nEnd = aStocks.rows.length; }
 if (aStocks.rows) {
  var j = 0;
  for (i=nStart;i<nEnd;i++) {
   aTmp[j] = new Array("","");

   shortNm = (shortNames[aStocks.rows[i][0]]) ? shortNames[aStocks.rows[i][0]] : aStocks.rows[i][0];

   clsStkDn = (aStocks.rows[i][2] < 0) ? "<font style=color:cc0000;>" : "";
   aTmp[j] = new Array(quoteRedir + "?symbol="+aStocks.rows[i][0] + " target=_top",shortNm + "  " +aStocks.rows[i][1]+ " " +clsStkDn+ aStocks.rows[i][2] + "</font>&nbsp;&nbsp;");
//   aTmp[j] = new Array(quoteRedir + "%3Fsymbol%3D"+aStocks.rows[i][0] + " target=_top",shortNm + "  " +aStocks.rows[i][1]+ " " +clsStkDn+ aStocks.rows[i][2] + "</font>&nbsp;&nbsp;");
  j++
  }
 } else {
  for (i=nStart;i<nEnd;i++) {
   // symbol, price, change, hi, lo, name, volume, %chg
   aTmp[i] = new Array("","");

   shortNm = (shortNames[aStocks[i][0]]) ? shortNames[aStocks[i][0]] : aStocks[i][0];

   clsStkDn = (aStocks[i][2] < 0) ? "<font style=color:cc0000;>" : "";
   aTmp[i] = new Array(quoteRedir + "?symbol="+aStocks[i][0] + " target=_top", shortNm + "  " +aStocks[i][1]+ " " +clsStkDn+ aStocks[i][2] + "</font>&nbsp;&nbsp;");
  }
 }
 return aTmp;
}

   mrktTicker = cnvrtQuote2List(quotedata,0, 6);
   mrktTicker2 = cnvrtQuote2List(quotedata,7);

   mrktTicker.appFmt = 4;
   mrktTicker.ID = "mrktTicker";
   mrktTicker.appWidth=649;
   mrktTicker.copyBorder = 0;
   mrktTicker.bPrintable=0;
   mrktTicker.nInterval = 2;
