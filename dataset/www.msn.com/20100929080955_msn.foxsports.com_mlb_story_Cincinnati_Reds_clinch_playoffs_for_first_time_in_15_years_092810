                        function unscapeHTML(text){
                            var newtext = text;
                            var pos=newtext.indexOf("&#34;");
                            while (pos>=0){
                                newtext = newtext.replace("&#34;", "\"");
                                pos=newtext.indexOf("&#34;");
                            }
                            pos=newtext.indexOf("&#39;");
                            while (pos>=0){
                                newtext = newtext.replace("&#39;", "'");
                                pos=newtext.indexOf("&#39;");
                            }
                            return newtext;
                        }
                    </script>

