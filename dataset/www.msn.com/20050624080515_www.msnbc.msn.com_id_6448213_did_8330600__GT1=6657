        tblMain.style.display = "block";
        
        var divList = tdCont.getElementsByTagName("div");
        for (i=0; i<divList.length; i++)
        {
            if (divList[i].className.indexOf("w300") > 0)
                divList[i].style.width = "300px";
        }
        
        var divDCol = document.getElementById("DCol");
        var divPD = divDCol.parentNode;
        
        divDCol.style.width = "160px";
        divDCol.style.backgroundColor = "#FFF";
        divDCol.style.textAlign = "center";
        divDCol.style.fontSize = "11pt";
        
        if (navigator.userAgent.indexOf("MSIE") < 0 || navigator.userAgent.indexOf("Opera") > 0)
        {
            divDCol.style.position = "relative";
            divDCol.style.left = "500px";
            divDCol.style.margin = "0 auto";
            
            if (navigator.userAgent.indexOf("Opera") > 0)
                divDCol.style.top = 1-divFoot.offsetTop + "px";
            else
                divDCol.style.top = 100 - divFoot.offsetTop + "px";
            
            divPD.removeChild(divDCol);
            
            //divPage.insertBefore(divDCol, divPage.firstChild);
            //bodyRef.insertBefore(divDCol, divWrap);
            //bodyRef.appendChild(divDCol);
            
            bodyRef.insertBefore(divDCol, null);
            //divFoot.style.height = "300px";
            //divFoot.style.overflow = "hidden";
            //divFoot.appendChild(divDCol);
            
//            var sCheckAd = document.createElement("span");
//            sCheckAd.style.visibility = "hidden";
//            divDCol.appendChild(sCheckAd);            
//            
//            var adHeight = sCheckAd.offsetTop - divDCol.offsetTop;
//                        
//            var sBottom = document.createElement("span");
//            sBottom.innerHTML = "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa";
//            //sBottom.style.visibility = "hidden";
//            
//            bodyRef.appendChild(sBottom);
//            sBottom.style.marginTop = -300 + "px";
            //divFoot.style.marginBottom = -adHeight + "px";
            //divFoot.style.marginBottom = "200px";
            
            //alert("adHeight:  " + adHeight);
        }
        else
        {
            try
            {
                //divDCol.style.cssFloat = "right";
                divDCol.style.styleFloat = "right";
                divDCol.style.position = "relative";
                divDCol.style.left = "180px";
            }catch(e){}
            divPage.insertBefore(divDCol, divPage.firstChild);
        }
        
        /*
        var newDCol = divPD.removeChild(divDCol);
        
