><span class="headlineList2"><a class="linkBlack" href="/web/20051103071537/http://msnbc.msn.com/id/9875670/" id="gted" CE="8">Curry: Senate has clear choice in Alito</a></span></div></div></span><div style="width:100%;height:16;"><div style="float:left;width:25;height:100%;"></div><div class="textHang"><span class="bulletRedSmall">• </span><span class="headlineList2"><a class="linkBlack" href="/web/20051103071537/http://msnbc.msn.com/id/3032553/">Politics Section Front</a></span></div></div><span class="textSmall"> </span></td></tr></table><table width="100%" cellspacing="0" cellpadding="0" class="boxF_3053751"><tr><td class="boxFI_3053751"><div class="textSmall"></div></td></tr></table></div><script>
			function nextStory(){
				var sStoryArray = "9900318^White House defends decision to go to war|9904528^WP: Food stamp cuts on the table|9887457^Minority leader sparks Senate standoff|9890833^WP: GOP’s DeLay hangs on to power|9898912^Guiliani touts alternative ed for poor|9891980^Colo. voters give up billions in tax refunds|9898557^Democratic senator lauds Alito|9875670^Curry: Senate has clear choice in Alito|";
				if(sStoryArray.charAt(sStoryArray.length)=="|"){
					sStoryArray = sStoryArray.substr(0,sStoryArray.length-1);
				}
				var aStoryArray = sStoryArray.split("|");
				if(window.partID){}else{partID='';}
				if (curDoc != '' && sStoryArray.indexOf(curDoc)>-1){
					curDom = document.getElementById(curDoc);
					curDom.style.backgroundColor = '#dddddd';
					curDomIH = curDom.innerHTML;
					curDom.innerHTML = curDomIH.substr(0,52) + '<img src="/images/arrowCurrentStory.gif" border="0" width="16" height="16">' + curDomIH.substr(52,curDomIH.length);
					for (i = 0; i < aStoryArray.length; i++) {
						var aCurStory = aStoryArray[i].split("^");
						var storyHref = aCurStory[0];
						var storyHL = aCurStory[1];
						if(storyHref == curDoc){
							var nxtStory = aStoryArray[i+1];
						}
					}
					if (nxtStory){
						var nxtStoryArray = nxtStory.split("^");
						var storyHref = nxtStoryArray[0];
						if (!isNaN(storyHref)){
							var storyHL = nxtStoryArray[1];
							if (partID){
								var nxtHref = 'href="/id/'+partID+'/did/'+storyHref+'"';}
							else{
								var nxtHref = 'href="/id/'+storyHref+'/"';}
							var nxtText = document.getElementById("nextText");
							var nxtButton = document.getElementById("StoryNextButton");
							var replaceStart = nxtButton.innerHTML.indexOf("href");
							nxtText.innerHTML = storyHL;
							nxtButton.innerHTML = nxtButton.innerHTML.replace(nxtButton.innerHTML.substring(replaceStart,replaceStart+19),nxtHref);
						}
					}
				}
			}
			nextStory();
