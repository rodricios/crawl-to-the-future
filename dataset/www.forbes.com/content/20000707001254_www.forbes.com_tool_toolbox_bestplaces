function DoSubmit() {
	iquery = document.SearchForm.i.value;
        //alert(iquery)
	if ((iquery.length > 253) || (iquery == "") || (iquery == "Search Term") || (iquery == "Enter term and select search category below"))  {
		window.location = "/web/20000815054952/http://www.forbesfinder.com";
                return false;
	} else {
	if (document.SearchForm.Sopt[0].checked==1) {
		document.SearchForm.action = "/web/20000815054952/http://www.forbesfinder.com/forbessearch/search.asp";
		document.SearchForm.q1.value = document.SearchForm.i.value;
		document.SearchForm.MT.value = document.SearchForm.i.value;
		document.SearchForm.RD.value = "DM";
	} else {
		document.SearchForm.action = "/web/20000815054952/http://www.forbesfinder.com/WebSearch/finder/forbesfinder.asp";
		document.SearchForm.q1.value = document.SearchForm.i.value;
		document.SearchForm.MT.value = document.SearchForm.i.value;
		document.SearchForm.RD.value = "AN";
	}	
	document.SearchForm.submit();
	return true;
	}
}


function SubmitUrl1() 
{ 
  
var urlStr = '/web/20000815054952/http://markets2.forbes.com/Stock_delay_report.asp?Symbol='+document.Inputform.Symbol.value+'&Timespan='+document.Inputform.Timespan.value;
var newWindow=window.open(urlStr,"_top");
