//function to pop up description boxes
function popup(type) { 
    remote = window.open('', 'TheRemote', 'toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=yes,resizable=no,width=500,height=100'); 
    remote.focus();
    if (remote != null) { 
    if (remote.opener == null) { 
        remote.opener = self; 
        } 
    if (type == '1') { 
		remote.location.href = '/tool/toolbox/bestplaces/00/1.htm'; 
	}
	if (type == '4') { 
		remote.location.href = '/tool/toolbox/bestplaces/00/4.htm'; 
	}

	if (type == '0') { 
		remote.location.href = '/tool/toolbox/bestplaces/00/0.htm'; 
	}									 
   }  
} 
</script>



