function writedate(){
	var m = new Date();
	var d = m.getDate();
	var y = m.getFullYear();
	var mo = m.getMonth(); 
	if (mo==0){	
		monthname = "January";
	} else if (mo==1){
		monthname = "February";
	} else if (mo==2){
		monthname = "March";
	} else if (mo==3){
		monthname = "April";
	} else if (mo==4){
		monthname = "May";
	} else if (mo==5){
		monthname = "June";
	} else if (mo==6){
		monthname = "July";
	} else if (mo==7){
		monthname = "August";
	} else if (mo==8){
		monthname = "September";
	} else if (mo==9){
		monthname = "October";
	} else if (mo==10){
		monthname = "November";
	} else if (mo==11){
		monthname = "December";
	}
	document.write(monthname + " " + d + ",  " + y);
}


function clearInputField(inputField) {
	inputField.value = "";
	}

