	function submitSearch() {

		if (document.searchOptions.searchType[0].checked) {
    		return true;
		} else if (document.searchOptions.searchType[1].checked) {
      		var gotoURL = "";
			var userInput = escape( document.Search.MT.value );

      		if( (userInput.length == 5) && ( (userInput.charAt(4) == "X") || (userInput.charAt(4) == "x") ) ) {
         		gotoURL = "/funds/Tearsheet.jhtml?tkr=" + userInput.toUpperCase();
      		} 
      		else {
         		gotoURL = "/finance/mktguideapps/compinfo/CompanyTearsheet.jhtml?tkr=" + userInput;
      		}
			
      		window.location = gotoURL;
		} 

    	return false;
	}
</script>

