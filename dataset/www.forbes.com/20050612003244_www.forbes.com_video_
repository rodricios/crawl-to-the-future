}

// Function to extract proper values from various field types
function getFormFieldValue(obj) {
      var cookie_value = '';
      var objType = new String(obj.type);
      switch(objType.toLowerCase()) {
	case "undefined" :
	  // a.k.a. radio field.
	    for (var i = 0; i < obj.length; i++) {
	      if (obj[i].checked) cookie_value = obj[i].value;
	    }
