	}
	cookie_value += ']';
	break;
	default :
         // We assume all other fields will have
	 // a valid obj.name and obj.value
	   cookie_value = obj.value;
      }
      return cookie_value;
}
function findCookie (name) {  
	//checking for an existing cookie

	var arg = name + "=";  
	var alen = arg.length;  
	var clen = document.cookie.length;  
	var i = 0;  

	while (i < clen) {    
		var j = i + alen; 

                if (document.cookie.substring(i, j) == arg)      
			return findCookieValue (j);  
		i = document.cookie.indexOf(" ", i) + 1;   

		if (i == 0) break;   
	}  
	return null;
}

function findCookieValue(offset) {
	//findCookie function uses this func

	var endstr = document.cookie.indexOf (";", offset);
	if (endstr == -1) {
           endstr = document.cookie.length;
        }
	return unescape(document.cookie.substring(offset, endstr));
}

