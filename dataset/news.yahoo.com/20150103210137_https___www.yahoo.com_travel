e":["#6891c4","#6669c6","#c9b294","#c7948d","#7479bc"],"tags":[{"tag":"Hotels","title":"Hotels","editor_info":{"promotion_date":1414769057394},"ts_update":1420314235,"bg_color":"#ffffff","text_color":"#000000","td_applet_type":"booking","suppress_articles":false,"display_name":"Hotels","alias_type":"NONE","ad_system_key":"22990489","allow_trailing":false,"name":"Hotels","slug":"hotels","count":200},{"tag":"atlantic-city","obj_type":"travel-guide","name":"Atlantic City","slug":"travel-guide:atlantic-city","count":5}],"url_slug":"atlantic-city-looks-for-better-luck-in-2015-106651640602","makelive_date":1420099257685,"ts_update":1419982451,"ccm_id":"270679ee-27dd-36d5-8637-6453ab7d95cf","page_date":1420099257684,"attribution":{"display_name":"Associated Press","url":{"href":"http:\u002F\u002Fbigstory.ap.org\u002F","rel":"alternate","type":"text\u002Fhtml"}},"image_sizes":[{"type":"ORIG","sizes":[{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w175\u002F24352c39217cb4ef63e892374084ec20ce544796.jpg","type":"image\u002Fjpeg","width":175,"height":131,"size_type":"175","formats":{}},{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w200\u002F24352c39217cb4ef63e892374084ec20ce544796.jpg","type":"image\u002Fjpeg","width":200,"height":150,"size_type":"200","formats":{}},{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w250\u002F24352c39217cb4ef63e892374084ec20ce544796.jpg","type":"image\u002Fjpeg","width":250,"height":188,"size_type":"250","formats":{}},{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w300\u002F24352c39217cb4ef63e892374084ec20ce544796.jpg","type":"image\u002Fjpeg","width":300,"height":225,"size_type":"300","formats":{}},{"url":"https:\u002F\u002Fs.yimg.com\u002Fcd\u002Fresizer\u002F2.0\u002FFIT_TO_WIDTH-w350\u002F24352c39217cb4ef63e892374084ec20ce544796.jpg","type":"image\u002Fjpeg","width":350,"height":263,"size_type"