                onPosMsg: function(cmd, pos, msg) {
                    try { // onPosMsg swallows errors, so let's log them
                        if (cmd === 'cmsg') {
                            console.log(arguments);
                            YMedia.one(YMedia.config.win).fire('splashmsg', {
                                pos: pos,
                                msg: msg
                            });
                        } else if (cmd === 'in-view') {
                            console.log(arguments);
                            YMedia.Af.Beacon.info('yosaic', {
                                code: 'darlaViewableImpression',
                                pos: pos
                            });
                        }
                    } catch(e) {
                        console.error(e);
                        throw e;
                    }
                },
                onFinishParse: function(eventName, response) {
                    try { // onFinishParse swallows errors, so let's log them
                        console.log(arguments);
                        YMedia.one(YMedia.config.win).fire('splashfinishparse', {
                            eventName: eventName,
                            response: response
                        });
                    } catch(e) {
                        console.error(e);
                        throw e;
                    }
                },
                onFinishPosRender: function(pos) {
                    if (
                        window.YAFT &&
                        window.YAFT.isInitialized() &&
                        window.performance !== undefined &&
                        window.performance.now !== undefined &&
                        (
                            pos === 'SPL' ||
                            pos === 'SPL2' ||
                            pos === 'LREC'
                        )
                    ) {
                        window.YAFT.triggerCustomTiming(pos + '-wrapper', '', window.performance.now());
                    }
                }
            };

            // TODO(gjones) temporary scroll tracking to attempt to isolate darla issues!
            (function() {
                var g_scroll_timer_id = 0;
                window.g_last_scroll_time = null;
                window.g_is_scrolling = false;

                function handle_scroll_done() {
                    if (g_scroll_timer_id) {
                        clearTimeout(g_scroll_timer_id);
                        g_scroll_timer_id = 0;
                        window.g_is_scrolling = false;
                    }
                }

                function handle_scroll() {
                    if (g_scroll_timer_id) {
                        clearTimeout(g_scroll_timer_id);
                        g_scroll_timer_id = 0;
                    }
                    window.g_last_scroll_time = (new Date()).getTime();
                    window.g_is_scrolling = true;
                    g_scroll_timer_id = setTimeout(handle_scroll_done, 500);
                }

                try { // don't bother with ie8
                    window.addEventListener('scroll', handle_scroll);
                } catch(e) {}
            })();
        
    </script>

