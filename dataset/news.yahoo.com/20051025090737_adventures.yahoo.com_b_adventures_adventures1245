<script language="Javascript" src="/web/20051026235937js_/http://us.js2.yimg.com/us.js.yimg.com/lib/common/utils/position_0.10.0.js"></script>

<script language="Javascript">
var oRO = {
  //addEvent cross browser event handling for ie5+ and ns6/mozilla by Scott Andrew
  addEvent : function (elm, evType, fn, useCapture) {
    if (elm.addEventListener) {
      elm.addEventListener(evType, fn, useCapture);
      return true;
    } else if (elm.attachEvent) {
      var r = elm.attachEvent('on' + evType, fn);
      return r;
    } else {
      elm['on' + evType] = fn;
    }
  },
  init: function() {
    if (!document.getElementsByTagName || 
        !document.getElementById)
      return;
    var oNav = document.getElementById('rbnavlist');
    if (!oNav)
      return; 
    oRO.listLinks = oNav.getElementsByTagName('a');
    for (var x=0; x < oRO.listLinks.length; x++) {
      //attach the mouse over and mouse out events to the links
      oRO.addEvent(oRO.listLinks[x], 'mouseover', oRO.mOver, false);
      oRO.addEvent(oRO.listLinks[x], 'mouseout', oRO.mOut, false);
      oRO.listLinks[x].hoverDiv = document.getElementById('day'+(x+1));
      oRO.listLinks[x].hoverDiv.opened = false;
      oRO.addEvent(oRO.listLinks[x].hoverDiv, 'mouseover', oRO.hoverMOver, false);
      oRO.addEvent(oRO.listLinks[x].hoverDiv, 'mouseout', oRO.hoverMOut, false);
      
    }
  },
  
  mOver: function(e) {
    var oLink;
    if (e && e.target)
      oLink = e.target;
    if (window.event && window.event.srcElement)
      oLink = window.event.srcElement;
    if (!oLink) 
      return;
    if (oLink.nodeType == 3) {
      oLink = oLink.parentNode;  
    }
    oRO.closeAllHovers();
    oLink.hoverDiv.style.position = 'absolute';
    var offset = (oLink.parentNode.clientWidth) ? (oLink.parentNode.clientWidth/2 -10) : 25; //hack to approximate for browsers that dont return clientWidth
    oLink.hoverDiv.style.left = ( ygPos.getX(oLink) - (100 - offset) )+ 'px';
    oLink.hoverDiv.style.top = (ygPos.getY(oLink)) + 'px';
    oLink.hoverDiv.style.display = 'block';
    if (oRO.timeOut) window.clearTimeout(oRO.timeOut);
    oLink.hoverDiv.opened = true;
  },
  startHoverClose:function() {
    oRO.timeOut = window.setTimeout('oRO.closeAllHovers()', 1000); 
  },
  mOut: function(e) {
    var oLink;
    if (e && e.target)
      oLink = e.target;
    if (window.event && window.event.srcElement)
      oLink = window.event.srcElement;
    if (!oLink) 
      return;
    if (oLink.nodeType == 3) {
      oLink = oLink.parentNode;  
    }
    oRO.startHoverClose();
    oLink.hoverDiv.opened = false;
  },
  hoverMOver: function(e) {
    var oLink;
    if (e && e.target)
      oLink = e.target;
    if (window.event && window.event.srcElement)
      oLink = window.event.srcElement;
    if (!oLink) 
      return;
    if (oLink.nodeType == 3) {
      oLink = oLink.parentNode;  
    }
    if (oRO.timeOut) window.clearTimeout(oRO.timeOut);
    oLink.opened = true;
  },
  hoverMOut : function(e) {
    var oLink;
    if (e && e.target)
      oLink = e.target;
    if (window.event && window.event.srcElement)
      oLink = window.event.srcElement;
    if (!oLink) 
      return;
    if (oLink.nodeType == 3) {
      oLink = oLink.parentNode;  
    }
    oRO.startHoverClose();
    oLink.opened = false;
  },
  closeAllHovers: function() {
    for (var x=0; x<oRO.listLinks.length; x++) {
      if (!oRO.listLinks[x].hoverDiv.opened) {
        oRO.listLinks[x].hoverDiv.style.display='none';
        oRO.listLinks[x].hoverDiv.opened = false;
      }
    }
  }
}


oRO.addEvent(window, 'load', oRO.init, false);
</script>
