function rs(n,u,w,h) {
remote = window.open(u, n, 'width=' + w + ',height=' + h +',resizable=no,scrollbars=no');
if (remote != null) {
if (remote.opener == null)
remote.opener = self;
window.name = 'yahooNewsRoot';
remote.location.href = u;
}
}

