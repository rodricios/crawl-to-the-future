		YUI().use('event', 'node', function (Y) {

			Y.on("domready", function(){
			
				var partner = Y.one('#partner-div').getAttribute('partner-name');
				
				/* ============== Article show-hide ================== */
					Y.all('.article .mc').addClass('mch');
					Y.all('.article .rm').on( 'click', function( e )  {
					if(e.target.get("tagName")!="A"){var exid = Y.one("#" + (e.target.ancestor().get("href").split("#")[1]));}
					else {var exid = Y.one("#" + (e.target.get("href").split("#")[1]));}
						var cexi = exid.get("className");
						if(cexi.indexOf("mch") > 0) {exid.removeClass("mch");exid.addClass("mcs");}
						else {exid.addClass("mch");exid.removeClass("mcs");}
						e.preventDefault();
					});


				
				if (Y.one('title') != null && Y.one('title').getHTML() != null) {
					// remove encoded <span></span> tags from title
					document.title = Y.one('title').getHTML().replace(/(<([^>]+)>)/ig,"");
				}

				 var YWATracker = YWA.getTracker("1000379873063");

				 		  YWATracker.setCF(8, "Help Central");
				 		  YWATracker.setCF(9, "Help Central");
				 		  YWATracker.setCF(10, "AMER");
				 		  YWATracker.setCF(11, "us");
				 		  YWATracker.setCF(12, "inquira");
				 		  YWATracker.setCF(2, "en-US");
				 		  YWATracker.setCF(1, "");
				 		  YWATracker.setCF(3, "B");
				 		  YWATracker.setCF(59, partner);
				 		  
