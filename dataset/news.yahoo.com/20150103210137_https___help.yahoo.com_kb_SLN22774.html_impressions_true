<script type="text/javascript">
	function isValidSearch() {
		var txtSearchHelp = document.getElementById('searchInput').value;
		return txtSearchHelp.trim().length > 0 && txtSearchHelp !='Search Help';
	}
	function blurSearchHelp()
	{
		if(document.getElementById('searchInput').value==''){
			document.getElementById('searchInput').value='Search Help';
		}
	}
	function focusSearchHelp()
	{
		if(document.getElementById('searchInput').value=='Search Help'){
			document.getElementById('searchInput').value='';
		}
	}
	
</script>
