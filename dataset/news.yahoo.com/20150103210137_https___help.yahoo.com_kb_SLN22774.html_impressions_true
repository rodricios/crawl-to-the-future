<script>
	YUI().use('event', 'node', 'cookie', 'json-parse', 'json-stringify', function (Y) {
	
		Y.on('domready', function(){
	
			// store recently viewed articles in cookie
			var articleId = 'SLN22774';
	
			var articleTitle = Y.one('title').getHTML().replace(/(<([^>]+)>)/ig,"");
			var token = 'null';

			// impressions=false for clicks from recently viewed list
			// TODO perhaps pass in viewlocale and actp?
			var recentArticle = '<a title="' + articleTitle + '" href="index?page=content&id=' + articleId + '&impressions=false&actp=RECENT">' + articleTitle + '</a>';
	
			//var recentlyViewed = $.evalJSON($.cookie('recentlyViewed'));
			var recentlyViewed = Y.JSON.parse(Y.Cookie.get('recentlyViewed'));
	
	
			if (typeof recentlyViewed != 'object' || recentlyViewed == null) {
				recentlyViewed = [];
			}
	
			// Only add this item to the recently viewed list if it does not already exist
			var addItem = true;
			for (var i=0; i<recentlyViewed.length; i++) {
				if (recentlyViewed[i] == recentArticle) {
					addItem = false;
				}
			}
			if (addItem) {
				recentlyViewed.push(recentArticle);
			}
			if (recentlyViewed.length >= 5) {
				recentlyViewed.shift();
			}
	
			//$.cookie('recentlyViewed', $.toJSON(recentlyViewed), {expires: 30, path: '/'});
			Y.Cookie.set('recentlyViewed', Y.JSON.stringify(recentlyViewed), {expires: 30, path: '/'});
	
			var chCat = "SLN22774";
			chCat = chCat.substring(0,2);
	
			captureArticleClickWithBucketInfo(chCat, "SLN22774", articleTitle);
			
			
		    Y.one('#feedback').hide();
		
			var clickyes = function(e){
				var token = 'null';
				Y.one('#feedback').show();
				Y.one('.confirm').setHTML('Thank you! 	Your feedback has successfully been submitted.');
				Y.one('#feedbackReasons').hide();
				Y.one('#fbk-btn').hide();
				Y.all('#ratingyes,#ratingyesicon,#ratingno,#ratingnoicon').detach();
				// YWA Call for Yes			

				if( token != 'null'){
					fireAction( "THUMB_UP" );	
				}
				
				sendYWAAction(5);
				
			}

			var clickno = function(e){
				Y.one('#feedback').show();
				Y.all('#ratingyes,#ratingyesicon,#ratingno,#ratingnoicon').detach();
				
				// YWA Call for No
				var token = 'null';	
				if( token != 'null'){
		            fireAction( "THUMB_DOWN" );
		         }
				
				 sendYWAAction(6);
		        
			}
			
			var clicksendfb = function(e){
				Y.one('#feedback').show();
				Y.one('.confirm').setHTML('Thank you! 	Your feedback has successfully been submitted.');
				Y.one('#feedbackReasons').hide();
				Y.one('#fbk-btn').hide();
				Y.all('#ratingyes,#ratingyesicon,#ratingno,#ratingnoicon').detach();
				// YWA Call for Feedback Button				
				var value = Y.all('[name=radioartrating]:checked').get('value');						
				sendYWAActionCF(26,17, value);
			}

			Y.one('#ratingyes').delegate('click', clickyes, 'div');
			Y.one('#ratingyesicon').delegate('click', clickyes, 'div');
			Y.one('#ratingno').delegate('click', clickno, 'div');
			Y.one('#ratingnoicon').delegate('click', clickno, 'div');
			Y.one('#fbk-btn').delegate('click', clicksendfb, 'div');
		
		});

	});
</script>
<div class="clear"></div>
<script>

function sendYWAActionCF(actionnumber, customFieldID, customFieldValue) {		
	  var platform = "inquira";
	  var YWATracker = YWA.getTracker("1000379873063");
	  YWATracker.setCF(12, platform);
	  YWATracker.setCF(2, "en-US");
	  YWATracker.setCF(customFieldID, customFieldValue.toString());
	  YWATracker.setAction(actionnumber);
	  YWATracker.submit_action();
	}
</script>
</div>
