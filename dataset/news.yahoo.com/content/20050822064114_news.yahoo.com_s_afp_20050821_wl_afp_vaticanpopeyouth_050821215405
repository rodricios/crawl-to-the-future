function validate_alerts()
{
  var valid=false;
  var theIncludes = document.getElementsByName("include");
  for(var i=0; i<theIncludes.length; i++)
    if (theIncludes[i].checked)
      valid=true;
  if (!valid)
    alert( "You need to check at least one keyword before creating an alert.");
  return valid;
}
