            require({ baseUrl: '/web/20150105205010/http://static.bbci.co.uk/travel/1.5.2/modules/'}, ['jquery-1'], function($) {
                $(document).ready(function() {
                    var pinItButton = $('.pin-it-button');
                    if(pinItButton){
                        pinItButton.click(function(e){
                           var galleryImageSrc = $('#featuredGallery .images img:visible').attr('src');
                           galleryImageSrc =  galleryImageSrc.replace(/\/\d+_\d+\//i, "/600_0/");
                           galleryImageSrc =  galleryImageSrc.replace(/\/\d+-\d+\//i, "/600-0/");
                            if(galleryImageSrc){
                                var match = pinItButton.attr('href').match(/&media=(.*)&|&media=(.*)$/i);
                                var updatedHref = pinItButton.attr('href').replace(match[2], galleryImageSrc);
                                pinItButton.attr('href', updatedHref);
                            }
                        });
                    }
                });
            });
