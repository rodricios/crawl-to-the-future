<script type="text/javascript"> 
/* <![CDATA[ */
var favicon = "/web/20101203042435/http://animal.discovery.com/images/icons/favicon.ico", currentPage = window.location.href, pageTitle = document.title; 
initApp();

function initApp() { 
        try {
                if ( window.external.msIsSiteMode()) { 
                    addJumpList(); 
                    addButtons();
                } 
        }
        catch (err){}
} 

function addWebApp() {
        try {
                if (document.addEventListener) {
                    document.addEventListener('mssitemodeinstalled', siteModeInstalled, false);
                }
                else {
                    document.attachEvent('onmssitemodeinstalled', siteModeInstalled);
                }
                window.external.msAddSiteMode();
        }
        catch (err){}
} 

function siteModeInstalled() {
        //window.close();
} 

function addJumpList() { 
        try {
                window.external.msSiteModeClearJumpList(); 
                window.external.msSiteModeCreateJumplist("Animal Planet Favorites"); 
                window.external.msSiteModeAddJumpListItem("Cats 101", "/web/20101203042435/http://animal.discovery.com/tv/cats-101/", favicon); 
                window.external.msSiteModeAddJumpListItem("Dogs 101", "/web/20101203042435/http://animal.discovery.com/tv/dogs-101/", favicon); 
                window.external.msSiteModeAddJumpListItem("Monsters Inside Me", "/web/20101203042435/http://animal.discovery.com/tv/monsters-inside-me/", favicon);
                window.external.msSiteModeAddJumpListItem("River Monsters", "/web/20101203042435/http://animal.discovery.com/tv/river-monsters/", favicon); 
                window.external.msSiteModeAddJumpListItem("Whale Wars", "/web/20101203042435/http://animal.discovery.com/tv/whale-wars/", favicon); 
                window.external.msSiteModeShowJumpList();
        } 
        catch (err){}
}

var btnOne, btnTwo, btnThree; 
function addButtons() {  
        try {
            btnOne = window.external.msSiteModeAddThumbBarButton("/web/20101203042435/http://static.discoverymedia.com/shared/images/icons/social-facebook.ico", "Share on Facebook"); 
            btnTwo = window.external.msSiteModeAddThumbBarButton("/web/20101203042435/http://static.discoverymedia.com/shared/images/icons/social-twitter.ico", "Tweet"); 
            btnThree = window.external.msSiteModeAddThumbBarButton("/web/20101203042435/http://static.discoverymedia.com/shared/images/icons/social-digg.ico", "Digg"); 
            btnFour = window.external.msSiteModeAddThumbBarButton("/web/20101203042435/http://static.discoverymedia.com/shared/images/icons/social-buzz.ico", "Social Buzz"); 
            
            if (document.addEventListener) { 
                document.addEventListener('msthumbnailclick', thumbnailclick, false); 
            } 
            else { 
                document.attachEvent('onmsthumbnailclick', thumbnailclick); 
            }          
            window.external.msSiteModeShowThumbBar(); 
        }
        catch (err){}                 
}

function thumbnailclick(btn) { 
        if (btn.buttonID == btnOne) { 
                window.location("/web/20101203042435/http://www.facebook.com/share.php?u=" + currentPage);         
        } 
        else if (btn.buttonID == btnTwo) { 
                window.location("/web/20101203042435/http://wd.sharethis.com/api/sharer.php?destination=twitter&url=" + currentPage + "&title=" + pageTitle);   
        } 
        else if (btn.buttonID == btnThree) { 
                window.location("/web/20101203042435/http://digg.com/submit?url=" + currentPage + "&title=" + pageTitle + "&topic=television"); 
        } 
        else if (btn.buttonID == btnFour) { 
                window.location("/web/20101203042435/http://buzz.yahoo.com/buzz?publisherurn=discovery_cha79&targetUrl=" + currentPage + "&amp;submitHeadline=" + pageTitle);   
        } 
} 
/* ]]> */
</script>


