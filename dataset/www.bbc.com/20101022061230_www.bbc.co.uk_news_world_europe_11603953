<h1 class="story-header">French police break key strike blockade near Paris</h1>
    
     
         
 
        		
		        
        <!--  Embedding the video player -->
<!--  This is the embedded player component -->


	
 

<div class="videoInStoryB">
    <div id="emp-11604092-378098" class="emp">
        		
