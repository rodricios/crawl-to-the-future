	<script type="text/javascript">
		Event.stopObserving(window,'load',function(){});

		Event.observe(window,'load',function() {
			$('sitelist-regions').hide();
			$('sitelist').observe('click',function() {
				var dd = this;
				Effect.toggle(
					'sitelist-regions',
					'blind',
					{duration: 0.3,
					afterFinish: function () { dd.className = (dd.className=='up')?'down':'up'; }
				});
				onclick = function(){return false;}
			});

			$$('#sitelist-regions ul li a').each(function(item) { 
				item.observe('click',function() {
					var values = item.getAttribute('cookie').split(',');
					var url = item.getAttribute('href');
					var anchor = url.replace('http://','').replace('/','');
					var domainStr = '';
					domainStr += (values[0] || '') + '__';
					domainStr += (values[1] || '') + '__';
					domainStr += (values[2] || '') + '__';
					domainStr += (anchor || '');
					createCookie('userLocation', domainStr, 30);
					//alert(readCookie('userLocation'));
					window.location.href=url;
				});
			});
		});
		//autoRedirect('');
	</script>
