            var siteAuto = false;
            var keywordsAuto = false;
            var zoneOverride = false;

            zone = 'travel_zone';
            zoneAuto = 'tvl_dest_moscow_index';
            siteAuto = 'bbccom.live.site.travel';


            if(/[?|&]zone=(\w*\/*\w+)(&|$)/.test(window.location.search)) {
                zone = RegExp.$1;
                zoneOverride = false;
            };

            if(/[?|&]zone=(/web/20100823052341/http:\/\/.+(\.bbc\.co\.uk){1}.*(bbc\.com){1}.*\.js)(&|$)/.test(window.location.search)) {
                if (RegExp.$1.indexOf("/../") === -1) {
                    zone = RegExp.$1;
                    zoneOverride = true;
                };
            };

            /* Added by Barlesque, so new static root can be passed post-load scripts. */
            BBC.adverts.setScriptRoot("/web/20100823052341/http://static.bbc.co.uk/bbcdotcom/0.3.14/script/");

