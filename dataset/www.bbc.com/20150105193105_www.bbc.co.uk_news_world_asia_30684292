<script type="text/javascript">
    (function () {
        var beta_cookie = {
            name: "ckps_tablet_beta",
            urlQueryString: "tablet-beta",
            optInValue: "i",
            optOutValue: "o"
        },

        preference_cookie = {
            name: "ckps_d",
            tabletValue: "t"
        },

        beta_domain = window.location.hostname.match(/com/) ? "/web/20150105170154/http://m.bbc.com" : "/web/20150105170154/http://m.bbc.co.uk",

        redirect_blacklist = [
            'market_data'
        ],

        getParameterByName = function (name) {
            var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
            return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
        },

        getCookieValue = function (cookie) {
            var cookies = document.cookie.split(";");
            var optInCookie;
            var optInCookieValue;
            var cookieIndex = cookies.length;

            while(cookieIndex--) {
                if(cookies[cookieIndex].indexOf(cookie.name) > -1) {
                    optInCookie = cookies[cookieIndex];
                    optInCookieValue = optInCookie.split('=').pop();

                    return optInCookieValue;
                }
            }

            return "";
        },

        setCookie = function (cookieToSet, valueToSet, days) {
            days = days || 120;
            var path = "/";
            var domain = window.location.hostname.match(/com/) ? ".bbc.com" : ".bbc.co.uk";
            var date = new Date();
            date.setTime(date.getTime() + days*24*60*60*1000);
            var expires = "; expires=" + date.toGMTString();
            document.cookie = cookieToSet + "=" + valueToSet + expires + "; path=" + path + "; domain=" + domain;
        },

        redirectToBetaDomain = function () {
            window.location = beta_domain + window.location.pathname;
        },

        instructedToOptOut = function() {
            return getParameterByName(beta_cookie.urlQueryString) === beta_cookie.optOutValue;
        },

        isOptedIn = function() {
            return getCookieValue(beta_cookie).indexOf(beta_cookie.optInValue) > -1;
        },

        isTabletPreferred = function() {
            return getCookieValue(preference_cookie).indexOf(preference_cookie.tabletValue) > -1;
        },

        isStaticDomain = function() {
            return window.location.host.match(/(nca|www\.int|www\.test|www\.stage|www)\.bbc\.(co\.uk|com)/);
        },

        isPathRedirectable = function() {
            var i = redirect_blacklist.length;
            while(i--) {
                if (window.location.pathname.indexOf(redirect_blacklist[i]) > -1) {
                    return false;
                }
            }
            return true;
        },

        execute = function() {
            if (instructedToOptOut()) {
                setCookie(beta_cookie.name, beta_cookie.optOutValue);
            }
            if (isOptedIn() && isTabletPreferred() && isStaticDomain() && isPathRedirectable()) {
                redirectToBetaDomain();
            }
        };

        return {
            execute: execute
        };

    })().execute();
</script>

<script type="text/javascript">

function BetaSite() {
  function setMobileCookie() {
    var d = new Date ();
    d.setDate(d.getDate() + 1);
    d = d.toUTCString();

    window.bbccookies.set('ckps_d=m;domain=.bbc.co.uk;path=/;expires=' + d );
    window.bbccookies.set('ckps_d=m;domain=.bbc.com;path=/;expires=' + d );
  }

  function isValidQueryString() {
    return (window.location.search.indexOf('view=beta') !== -1);
  }

  function isClientCapable() {
    return ('querySelector' in document && 'localStorage' in window && 'addEventListener' in window);
  }

  if (isClientCapable() && isValidQueryString()) {
    setMobileCookie();

    window.location.host = 'm.bbc.co.uk';
  }
}

BetaSite();

</script>


