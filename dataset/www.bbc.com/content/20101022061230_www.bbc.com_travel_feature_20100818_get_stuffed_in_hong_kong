        (function(){
            var zone = "3pt_zone_file";
            var zoneAuto = false;
            var siteAuto = false;
            var keywordsAuto = false;
            var zoneOverride = false;

            zone = 'travel_zone';
            zoneAuto = 'tvl_dest_china_hong_kong_content';
            siteAuto = 'bbccom.live.site.travel';
            keywordsAuto = {tvl_theme:"food_drink", tvl_dest:"hong_kong"};


            if(/[?|&]zone=(\w*\/*\w+)(&|$)/.test(window.location.search)) {
                zone = RegExp.$1;
                zoneOverride = false;
            };

            if(/[?|&]zone=(/web/20100905063607/http:\/\/.+(\.bbc\.co\.uk){1}.*(bbc\.com){1}.*\.js)(&|$)/.test(window.location.search)) {
                if (RegExp.$1.indexOf("/../") === -1) {
                    zone = RegExp.$1;
                    zoneOverride = true;
                };
            };

            /* Added by Barlesque, so new static root can be passed post-load scripts. */
            BBC.adverts.setScriptRoot("/web/20100905063607/http://static.bbc.co.uk/bbcdotcom/0.3.15/script/");

            BBC.adverts.init({
                domain: "www.bbc.com",
