<h1 class="story-header">Manx residents honour Sir Norman Wisdom</h1>
    
     
         
 
        		
		        
        <!--  Embedding the video player -->
<!--  This is the embedded player component -->


	
 

<div class="videoInStoryB">
    <div id="emp-11610351-61927" class="emp">
        		
        <noscript>
