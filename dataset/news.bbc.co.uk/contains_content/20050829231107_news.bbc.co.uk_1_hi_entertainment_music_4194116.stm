                    <div class="miiib">
    
	<!-- S ILIN -->
        
        
		<div class="arr">
			<a href="/web/20051001104129/http://news.bbc.co.uk/1/hi/in_pictures/4194372.stm" class="">In pictures: MTV music awards</a>
		</div>
        
	<!-- E ILIN -->
    



    
	<!-- S ILIN -->
        
        
		<div class="arr">
			<a href="/web/20051001104129/http://news.bbc.co.uk/1/hi/entertainment/music/4194540.stm" class="">Green Day: Punks for the masses</a>
		</div>
        
