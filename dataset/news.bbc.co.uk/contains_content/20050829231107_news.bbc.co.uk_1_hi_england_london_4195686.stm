                <tr>
                    <td valign="top" width="416">
                        
	
		
                    	<font size="2">
		
			<!-- S BO -->
<!-- S IBYL -->
<div class="mvb">


    <table border="0" width="416" cellpadding="0" cellspacing="0">
        <tr>
        <td valign="bottom">
            <div class="mvb">
                
                    
                    <span class="byl">
                        By Clark Ainsworth
                    </span>
                
                
                    <br />
                    <span class="byd">
