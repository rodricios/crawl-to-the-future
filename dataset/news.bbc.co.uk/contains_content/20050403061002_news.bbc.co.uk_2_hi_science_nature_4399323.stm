<!-- E IBYL -->


<P>
<!-- S IIMA -->
	
		<table cellspacing="0" align="right" border="0" width="203" cellpadding="0">
			<tr><td>
			<div>
				<img height="152" hspace="0" vspace="0" border="0" width="203" alt="Stansted Airport" src="/web/20051107050515im_/http://newsimg.bbc.co.uk/media/images/39640000/jpg/_39640853_stansted203.jpg" />
