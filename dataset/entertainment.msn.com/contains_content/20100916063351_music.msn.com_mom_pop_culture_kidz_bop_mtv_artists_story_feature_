function sendMail(){
 
 var bodyLine= encodeURI("I found this on MSN Movies and thought you might be interested in it! \r\n\r\n/web/20100913054024/http://tv.msn.com/mom-pop-culture/");
 
 parent.location.href="mailto:?subject=Mom & Pop Culture on MSN&body="+bodyLine;
 }


function bookmark(url,title){
alert('You are about to bookmark this page');
  if ((navigator.appName == "Microsoft Internet Explorer") && (parseInt(navigator.appVersion) >= 4)) {
  window.external.AddFavorite(url,title);
  } else if (navigator.appName == "Netscape") {
    window.sidebar.addPanel(title,url,"");
  } else {
    alert("Press CTRL-D (Netscape) or CTRL-T (Opera) to bookmark");
  }
}

</script>

