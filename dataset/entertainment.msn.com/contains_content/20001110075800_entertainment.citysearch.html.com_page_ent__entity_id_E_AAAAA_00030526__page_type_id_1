function mOvr(src,clrOver){
    if(!src.contains(event.fromElement)){
        src.style.cursor="hand"
        src.BGCOLOR=clrOver
    }
}

function mOut(src,clrIn){
    if(!src.contains(event.toElement)){
        src.style.cursor="default"
        src.BGCOLOR=clrIn
    }
}

function mClk(src){
    if(event.srcElement.tagName=="TD"){
        src.children.tags("A")[0].click()
    }
}

function validate(){
var zip=document.SearchStrip.postal_code.value
var key=document.SearchStrip.keywords.value

var zipmsg="Please enter a valid 5-number zip code, or select a neighborhood."
var keymsg="Please enter something to search."

    if(key!=""){
        key=key.toLowerCase()
        if(key.substring(key.length-1,key.length)=="s"){
            key=key.substring(0,key.length-1)
        }
        document.SearchStrip.alt_keywords.VALUE=key
        return(true)
    }
    else{
        alert(keymsg)
        document.SearchStrip.keywords.focus()
        return(false)
    }
}
