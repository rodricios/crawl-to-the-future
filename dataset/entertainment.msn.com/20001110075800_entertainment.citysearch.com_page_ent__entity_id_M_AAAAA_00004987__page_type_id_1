function validate(){
var zip=document.SearchStrip.postal_code.value
var key=document.SearchStrip.keywords.value

var zipmsg="Please enter a valid 5-number zip code, or select a neighborhood."
var keymsg="Please enter something to search."

    if(key!=""){
