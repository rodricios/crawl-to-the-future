										 <SCRIPT type=text/javascript> // To collapse long gallery descriptions: // // 1) In Gallery tool, wrap photo description in <div id="infoText"></div> //    and place the following empty div beneath: <div id="infoTextHolder"></div> // 2) Optional: Customize character count with collapsedInfoLength variable (default is 400) //    <script type="text/javascript">var collapsedInfoLength=500;</scr_pt>    (<--replace 'i') // var collapsedInfoLength; var divIT = document.getElementById("infoText"); var divITH = document.getElementById("infoTextHolder");  function collapseInfoText(collapsedInfoLength){     if (undefined == collapsedInfoLength){         collapsedInfoLength = 410;     }     if (divIT && divITH) {         divITH.innerHTML = divIT.innerHTML.substr(0,collapsedInfoLength) + "... <span class='readmore' onclick='expandInfoText()' onmouseover=\"this.style.textDecoration = 'underline';\" onmouseout=\"this.style.textDecoration = 'none';\">[Read More]</span>";         divIT.style.display="none";     } } function expandInfoText(){    if (divIT && divITH) {        divITH.innerHTML = divIT.innerHTML + "<p><span class='readmore' onclick='collapseInfoText(collapsedInfoLength)' onmouseover=\"this.style.textDecoration = 'underline';\" onmouseout=\"this.style.textDecoration = 'none';\">[Close]</span></p>";    } } collapseInfoText(collapsedInfoLength); </SCRIPT> 
									
