function getChartbeatDomain() {
    var domain = '', 
        edition = unescape(document.cookie).match('NYT-Edition=([^;]+)');

    if (edition && edition[1] && edition[1].indexOf("edition|GLOBAL") !== -1) {
        domain = "international.nytimes.com";
    } else {
        domain = "www.nytimes.com"
    }
    return domain;
}

