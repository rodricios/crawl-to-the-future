var the_date = new Date();
//Get all the elements and set up your owm the_time from scratch.
var the_year = the_date.getUTCFullYear();
var the_month = the_date.getUTCMonth();
var the_day = the_date.getUTCDate();
var the_hour = the_date.getUTCHours();
var the_minute = the_date.getUTCMinutes();
var grayline = 1;
//THIS FUNCTION GATHERS THE CURRENT TIME (YEAR,MONTH,DAY,HOUR,MINUTE)
var the_time=Date.UTC(the_year,the_month,the_day,the_hour,the_minute);
