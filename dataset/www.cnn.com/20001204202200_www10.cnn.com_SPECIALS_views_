<!-- analysis banner -->
<IMG SRC="/web/20001217001300im_/http://www7.cnn.com/SPECIALS/views/images/1999/top.banner.gif" ALT="y " HSPACE="0" VSPACE="0" BORDER="0" WIDTH="280" HEIGHT="24">
